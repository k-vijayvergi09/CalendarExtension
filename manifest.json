{
  "manifest_version": 3,
  "name": "Calendar Extension",
  "version": "1.0.0",
  "description": "A calendar extension with sidepanel view",
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Calendar Extension"
  }
} 